<table class="ui three column celled table">
  <thead>
    <tr>
      <th>Target Date</th>
      <th>Target Weight</th>
      <th>Status</th>
    </tr>
  </thead>
  <tbody>
    {{#each goals}}
      <tr>
        <td>{{dateString date}}</td>
        <td>{{weight}}</td>
        <td>{{status}}</td>
      </tr>
    {{/each}}
  </tbody>
</table>

